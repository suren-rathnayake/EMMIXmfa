year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
         title = "{EMMIXmfa}: Mixture Models with Component-Wise Factor Analyzers",
         author = c(person("Rathnayake", "Suren"),
                    person("Baek", "Jangsun"),
                    person("McLachlan", "Geoffrey"))
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=EMMIXmfa")

